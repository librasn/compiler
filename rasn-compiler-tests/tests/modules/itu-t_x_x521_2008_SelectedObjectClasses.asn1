}

isoTagType OBJECT-CLASS ::= {
  SUBCLASS OF  {top}
  KIND         auxiliary
  MAY CONTAIN  {tagOid | uiiFormat }
  ID           id-oc-isoTagType
}

countryNameForm NAME-FORM ::= {
  NAMES            country
